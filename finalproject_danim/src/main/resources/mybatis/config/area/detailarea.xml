<?xml version="1.0" encoding="UTF-8"?>
<!-- Sql Mapper -->
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="detailarea">
 	<select id="searchDetailArea" resultType="detailAreaVO">
 		select detailarea_name from detailarea
 		where area_name=#{value}
 	</select>
</mapper>